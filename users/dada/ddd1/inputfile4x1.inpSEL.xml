		<opacity>100, 10</opacity>
		<border>2</border>
		<borderColor>CCCCCC</borderColor>
	</chartScene>
	
<!-- GRID -->
	<grid>
		<vertical>.1</vertical>	
		<verticalColor>CCCCCC</verticalColor>	
		<horizontal>.1</horizontal>
		<horizontalColor>CCCCCC</horizontalColor>
	</grid>
	
